pipeline{
  agent any
  
  stages {

    stage("npm install"){
      steps{

        echo 'Executing npm . . .'
        nodejs('Node-18.14.0'){
          sh 'npm install'

        }
      }
    }
     stage('SonarQube Analysis') {
      steps{
         withSonarQubeEnv('scanner') {
      sh "${scannerHome}/bin/sonar-scanner"
    }

      }
    
  }

 stage("test"){
      steps{

        echo "Testingg . . ."
      }
    }
     stage("Deploy"){ 
      steps{

        echo "Deployingg. . ."
      }
    }
    
  }


}